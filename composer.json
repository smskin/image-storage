{
  "name": "smskin/image-storage",
  "description": "Image storage interface library",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "SMSkin",
      "email": "serg@msoinvest.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.1",
    "guzzlehttp/guzzle": "~6.0",
    "illuminate/support": "^5.5|^6",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "SMSkin\\ImageStorage\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SMSkin\\ImageStorage\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "SMSkin\\ImageStorage\\ServiceProvider"
      ]
    }
  }
}
